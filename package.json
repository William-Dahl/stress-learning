    "autoprefixer": "10.4.14",
    "cookies-next": "^2.1.1",
    "dotenv": "^16.0.0",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "next": "13.3.2",
    "payload": "^1.7.2",
    "postcss": "8.4.23",
    "qs": "^6.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-player": "^2.12.0",
    "slate": "^0.94.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
