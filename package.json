{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "start": "next start",
    "lint": "next lint",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:payload": "payload build",
    "copyfiles:media": "copyfiles -u 1 media/**/*.{svg,jpg,png} dist/media",
